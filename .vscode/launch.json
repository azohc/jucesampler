{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/Sampler/Builds/LinuxMakefile/build/Sampler",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/Sampler/Builds/LinuxMakefile/build/",
            "additionalSOLibSearchPath": "${workspaceFolder}/Sampler/Builds/LinuxMakefile/build",
            "environment": [
                {
                    "name": "LD_LIBRARY_PATH",
                    "value": "."
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "buildscript"
        }
        
    ]
}